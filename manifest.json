{
    "manifest_version": 2,
    "name": "T33nc0d3",
    "version": "0.1.0",
    "description": "Teencode đê anh bạn",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png"
        }
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "functions.js",
                "jquery-1.10.0.js",
                "jquery-ui.min.js",
                "owl.carousel.js",
                "clipboard.min.js"
            ],
            "css": ["popup.css"]
        }
    ]
}